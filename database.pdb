<o:TextTargetItem Id="o64">
<a:Name>SqlOptsQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, OPTIONS}

select
   u.user_name,
   t.table_name,
   &#39;in &#39;+ f.dbspace_name
from
   sys.sysuserperms u
   join sys.systab t on (t.creator = u.user_id)
   join sys.sysfile f on (f.file_id = t.file_id)
where
   f.dbspace_name &lt;&gt; &#39;SYSTEM&#39;
   and t.table_type in (1, 3, 4)
[  and t.table_name = %.q:TABLE%]
[  and u.user_name = %.q:OWNER%]</a:TextTargetItem.Value>
<a:Comment>SQL query to reverse object physical options</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o65">
<a:Name>SqlChckQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, CONSTNAME, CONSTRAINT}

select 
   u.user_name,
   t.table_name,
   k.constraint_name,
   case(lcase(left(h.check_defn, 5))) when &#39;check&#39; then substring(h.check_defn, 6) else h.check_defn end
from
   sys.sysconstraint k
   join sys.syscheck h on (h.check_id = k.constraint_id)
   join sys.systab t on (t.object_id = k.table_object_id)
   join sys.sysuserperms u on (u.user_id = t.creator)
where
   k.constraint_type = &#39;T&#39;
   and t.table_type in (1, 3, 4)
[  and u.user_name = %.q:OWNER%]
[  and t.table_name = %.q:TABLE%]
order by 1, 2, 3
</a:TextTargetItem.Value>
<a:Comment>SQL query to reverse object check constraints</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o66">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, TemporaryTable, PctFree, COMMENT}

select 
