   t.table_name, 
   case(t.table_type) when &#39;3&#39; then &#39;global&#39; when 4 then &#39;local&#39; else &#39;&#39; end,
   t.pct_free,
   r.remarks
from 
   sys.sysuserperms u
   join sys.systab t on (t.creator = u.user_id)
   left outer join sys.sysremark as r on (r.object_id = t.object_id)
where
    t.table_type in (1, 3, 4)
[  and u.user_name = %.q:OWNER%]
[  and t.table_name = %.q:TABLE%]
order by 1, 2</a:TextTargetItem.Value>
