<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, COLUMN, DTTPCODE, LENGTH, PREC, NOTNULL, @OBJTLABL, DOMAIN, DEFAULT, COMPUTE, Compressed}

select
   u.user_name,
   t.table_name,
   c.column_name,
   d.domain_name,
   c.width,
   c.scale,
   case(c.nulls) when &#39;Y&#39; then &#39;NULL&#39; else &#39;NOT NULL&#39; end,
   (select r.remarks from sys.sysremark r where r.object_id = c.object_id),
   x.type_name,
   case (c.column_type) when &#39;R&#39; then c.\[default\] else null end,
   case (c.column_type) when &#39;C&#39; then c.\[default\] else null end,
   case (c.\[compressed\]) when 1 then &#39;true&#39; else &#39;false&#39; end
from
   sys.sysuserperms u
   join sys.systab t on (t.creator=u.user_id)
   join sys.systabcol c on (c.table_id=t.table_id)
   left join sys.sysusertype x on (x.type_id = c.user_type)
   join sys.sysdomain d on (d.domain_id=c.domain_id)
where
   c.column_type in (&#39;C&#39;, &#39;R&#39;)
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
order by
   1, 2, c.column_id
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
