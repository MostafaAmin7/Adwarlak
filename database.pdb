<o:TextTargetItem Id="o83">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, TABLE, INDEX, UNIQUE, CLUSTER, CIDXLIST, COMMENT}

select
   u.user_name,
   t.table_name,
   i.index_name,
   case (i.\[unique\]) when 1 then &#39;unique&#39; else &#39;&#39; end,
   case when (i.index_id = t.clustered_index_id) then &#39;clustered&#39; else &#39;&#39; end,
   (select list(string(c.column_name, case (x.\[order\]) when &#39;D&#39; then &#39; desc&#39; else &#39; asc&#39; end), &#39;, &#39;)
    from sys.sysidxcol x join sys.syscolumn c on (c.table_id = x.table_id and c.column_id = x.column_id)
    where x.table_id = i.table_id and x.index_id = i.index_id) as coln,
   (select r.remarks from sys.sysremark r where r.object_id = i.object_id)
from
   sys.sysuserperms u
   join sys.systab t on (t.creator=u.user_id)
   join sys.sysidx i on (i.table_id=t.table_id)
where
   i.index_category = 3
   and i.\[unique\] != 2
[  and u.user_name=%.q:OWNER%]
[  and t.table_name=%.q:TABLE%]
order by 
   1, 2, i.index_id
</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
