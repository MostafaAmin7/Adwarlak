<o:TextTargetItem Id="o270">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, EVENT, ScheduledEvent, FullEventText}

select
   u.user_name,
   e.event_name,
   case(isnull(e.event_type_id, 0)) when 0 then &#39;true&#39; else &#39;false&#39; end,
   &#39;&#39;
from
   sys.sysevent e
   join sys.sysuserperms u on (u.user_id = e.creator)
[where u.user_name = %.q:SCHEMA%]
order by 1, 2</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o271">
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, EVENT, Enable, Database, Handler}

select
   u.user_name,
   e.event_name,
   case(e.enabled) when &#39;Y&#39; then &#39;true&#39; else &#39;false&#39; end,
   case(e.location) when &#39;A&#39; then &#39;all&#39; when &#39;C&#39; then &#39;consolidated&#39; when &#39;R&#39; then &#39;remote&#39; else &#39;&#39; end,
   e.source
from
   sys.sysevent e
   join sys.sysuserperms u on (u.user_id = e.creator)
where 1=1
[  and u.user_name = %.q:OWNER%]
[  and e.event_name = %.q:EVENT%]
</a:TextTargetItem.Value>
