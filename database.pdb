   isnull(p.source, p.proc_defn),
   (select r.remarks from sys.sysremark r where r.object_id = p.object_id)
from
   sys.sysprocedure p
where 1=1
[  and user_name(p.creator) = %.q:SCHEMA%]
[  and p.proc_name = %.q:PROC%]
