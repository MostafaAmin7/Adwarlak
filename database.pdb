<o:TextTargetItem Id="o276">
<a:Name>SqlScheduledEventQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, EVENT, ScheduledEvent, SchedulesText}

select
   u.user_name,
   e.event_name,
   &#39;true&#39;,
   (select 
      list(&#39;schedule &#39; + s.sched_name
      + ifnull(s.stop_time, 
         &#39; start time &#39;&#39;&#39; + convert(varchar(10), s.start_time) + &#39;&#39;&#39;&#39;,
         &#39; between &#39;&#39;&#39; + convert(varchar(10), s.start_time) + &#39;&#39;&#39; and &#39;&#39;&#39; + convert(varchar(10), s.stop_time) + &#39;&#39;&#39;&#39;
         )
      + ifnull(s.days_of_week, &#39;&#39;,
         &#39; on (&#39;
         + replace(trim(string(
            case(s.days_of_week &amp; 0x01) when 0 then &#39;&#39; else &#39;&#39;&#39;Sunday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x02) when 0 then &#39;&#39; else &#39;&#39;&#39;Monday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x04) when 0 then &#39;&#39; else &#39;&#39;&#39;Tuesday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x08) when 0 then &#39;&#39; else &#39;&#39;&#39;Wednesday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x10) when 0 then &#39;&#39; else &#39;&#39;&#39;Thursday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x20) when 0 then &#39;&#39; else &#39;&#39;&#39;Friday&#39;&#39; &#39; end,
            case(s.days_of_week &amp; 0x40) when 0 then &#39;&#39; else &#39;&#39;&#39;Saturday&#39;&#39; &#39; end)), &#39; &#39;, &#39;, &#39;)
         + &#39;)&#39;
         )
      + ifnull(s.days_of_month, &#39;&#39;,
         &#39; on (&#39; 
         + replace(trim(string(
             case(s.days_of_month &amp; 0x00000001) when 0 then &#39;&#39; else &#39;1 &#39; end,
             case(s.days_of_month &amp; 0x00000002) when 0 then &#39;&#39; else &#39;2 &#39; end,
             case(s.days_of_month &amp; 0x00000004) when 0 then &#39;&#39; else &#39;3 &#39; end,
             case(s.days_of_month &amp; 0x00000008) when 0 then &#39;&#39; else &#39;4 &#39; end,
             case(s.days_of_month &amp; 0x00000010) when 0 then &#39;&#39; else &#39;5 &#39; end,
             case(s.days_of_month &amp; 0x00000020) when 0 then &#39;&#39; else &#39;6 &#39; end,
             case(s.days_of_month &amp; 0x00000040) when 0 then &#39;&#39; else &#39;7 &#39; end,
             case(s.days_of_month &amp; 0x00000080) when 0 then &#39;&#39; else &#39;8 &#39; end,
             case(s.days_of_month &amp; 0x00000100) when 0 then &#39;&#39; else &#39;9 &#39; end,
             case(s.days_of_month &amp; 0x00000200) when 0 then &#39;&#39; else &#39;10 &#39; end,
             case(s.days_of_month &amp; 0x00000400) when 0 then &#39;&#39; else &#39;11 &#39; end,
             case(s.days_of_month &amp; 0x00000800) when 0 then &#39;&#39; else &#39;12 &#39; end,
             case(s.days_of_month &amp; 0x00001000) when 0 then &#39;&#39; else &#39;13 &#39; end,
             case(s.days_of_month &amp; 0x00002000) when 0 then &#39;&#39; else &#39;14 &#39; end,
             case(s.days_of_month &amp; 0x00004000) when 0 then &#39;&#39; else &#39;15 &#39; end,
             case(s.days_of_month &amp; 0x00008000) when 0 then &#39;&#39; else &#39;16 &#39; end,
             case(s.days_of_month &amp; 0x00010000) when 0 then &#39;&#39; else &#39;17 &#39; end,
             case(s.days_of_month &amp; 0x00020000) when 0 then &#39;&#39; else &#39;18 &#39; end,
             case(s.days_of_month &amp; 0x00040000) when 0 then &#39;&#39; else &#39;19 &#39; end,
             case(s.days_of_month &amp; 0x00080000) when 0 then &#39;&#39; else &#39;20 &#39; end,
             case(s.days_of_month &amp; 0x00100000) when 0 then &#39;&#39; else &#39;21 &#39; end,
             case(s.days_of_month &amp; 0x00200000) when 0 then &#39;&#39; else &#39;22 &#39; end,
             case(s.days_of_month &amp; 0x00400000) when 0 then &#39;&#39; else &#39;23 &#39; end,
             case(s.days_of_month &amp; 0x00800000) when 0 then &#39;&#39; else &#39;24 &#39; end,
             case(s.days_of_month &amp; 0x01000000) when 0 then &#39;&#39; else &#39;25 &#39; end,
             case(s.days_of_month &amp; 0x02000000) when 0 then &#39;&#39; else &#39;26 &#39; end,
             case(s.days_of_month &amp; 0x04000000) when 0 then &#39;&#39; else &#39;27 &#39; end,
             case(s.days_of_month &amp; 0x08000000) when 0 then &#39;&#39; else &#39;28 &#39; end,
             case(s.days_of_month &amp; 0x10000000) when 0 then &#39;&#39; else &#39;29 &#39; end,
             case(s.days_of_month &amp; 0x20000000) when 0 then &#39;&#39; else &#39;30 &#39; end,
             case(s.days_of_month &amp; 0x40000000) when 0 then &#39;&#39; else &#39;31 &#39; end,
             case(s.days_of_month &amp; 0x80000000) when 0 then &#39;&#39; else &#39;0 &#39; end)), &#39; &#39;, &#39;, &#39;)
         + &#39;)&#39;
         )
      + ifnull(s.interval_amt, &#39;&#39;,
         &#39; every &#39; + convert(varchar(10), s.interval_amt) + case(s.interval_units) when &#39;HH&#39; then &#39; hours&#39; when &#39;NN&#39; then &#39; minutes&#39; when &#39;SS&#39; then &#39; seconds&#39; end
         )
      + ifnull(s.start_date, &#39;&#39;, &#39; start date &#39;&#39;&#39; + cast(s.start_date as varchar(30)) + &#39;&#39;&#39;&#39;)
      , &#39;,&#39;)
      from sys.sysschedule s where s.event_id = e.event_id)
from
   sys.sysevent e
   join sys.sysuserperms u on (u.user_id = e.creator)
where e.event_type_id is null
[  and u.user_name = %.q:OWNER%]
[  and e.event_name = %.q:EVENT%]
order by 1, 2</a:TextTargetItem.Value>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
<o:TextTargetItem Id="o277">
<a:Name>SqlTypedEventQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, EVENT, TypedEvent, EventType}

select
   u.user_name,
   e.event_name,
   &#39;true&#39;,
   t.name
from
   sys.sysevent e
   join sys.sysuserperms u on (u.user_id = e.creator)
   join sys.syseventtype t on (t.event_type_id = e.event_type_id)
where e.event_type_id is not null
[  and u.user_name = %.q:OWNER%]
[  and e.event_name = %.q:EVENT%]
</a:TextTargetItem.Value>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
</o:TextTargetItem>
