<o:TextTargetItem Id="o251">
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{ WEBSERVICE, WEBSERVICETYPE, WEBSERVICEPATH, Prefix, COMMENT, WEBAUTHREQUIRED, WEBCNCTSECURED, WEBUSER }

select distinct
   case(w.service_type) when &#39;DISH&#39; then
      if (locate(service_name, &#39;/&#39;) &gt; 1) then
         substr(service_name, locate(service_name, &#39;/&#39;, -1)+1)
      else 
         service_name
      endif
   else
      case(locate(w.service_name, &#39;/&#39;))
         when 0 then w.service_type
         when locate(w.service_name, &#39;/&#39;, -1) then substr(w.service_name, 1, locate(w.service_name, &#39;/&#39;, -1)-1)
         else substr(w.service_name, locate(substr(w.service_name, 1, locate(w.service_name, &#39;/&#39;, -1)-1), &#39;/&#39;)+1, locate(w.service_name, &#39;/&#39;, -1)- locate(substr(w.service_name, 1, locate(w.service_name, &#39;/&#39;, -1)-1), &#39;/&#39;) - 1)
      end
   end as colnA,
   w.service_type,
   case when (w.service_type = &#39;DISH&#39;) then w.service_name
        when (locate(w.service_name, &#39;/&#39;) &gt; 1) then substr(w.service_name, 1, locate(w.service_name, &#39;/&#39;, -1)-1) 
        else &#39;/&#39;
   end as colnB,
   case(w.service_type) when &#39;DISH&#39; then w.parameter else NULL end,
   case(w.service_type) when &#39;DISH&#39; then w.remarks else NULL end,
   case(w.service_type) when &#39;DISH&#39; then w.auth_required else NULL end,
   case(w.service_type) when &#39;DISH&#39; then w.secure_required else NULL end,
   case(w.service_type) when &#39;DISH&#39; then u.user_name else NULL end
from 
   sys.syswebservice w
   left outer join sys.sysuserperms u on (u.user_id = w.user_id and w.service_type = &#39;DISH&#39;)
where service_type != &#39;SOAP&#39;
order by 1, 2, 3</a:TextTargetItem.Value>
<a:Comment>SQL query to list objects</a:Comment>
</o:TextTargetItem>
